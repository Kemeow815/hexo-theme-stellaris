.paginator-wrap
  //margin-top: 1.5rem
  display: flex
  justify-content: center
  align-items: center
  vertical-align: center
  font-size: 1rem
  font-weight: 700
  border-radius: $border-card
  overflow: hidden
//  box-shadow: 0 2px 8px 0px rgba(0, 0, 0, 0.03)
  .page-number 
    padding: 4px 8px
    border-radius: 6px
    //background: var(--block)
    margin: 0.5rem
    color: var(--text-p3)
    &:hover
      color: var(--theme-highlight)
  .space
    margin: 1rem
    color: var(--text-p2)
  .extend
    text-align: center
    background-size: contain
    width: 1rem
    height: 1rem
    background-origin: content-box
    background-clip: content-box
    &.next
      background-image: url("data:image/svg+xml;base64,")
    &.prev
      background-image: url('data:image/svg+xml,utf8,<svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M18 3C18.5128 3 18.9355 3.38604 18.9933 3.88338L19 4V20C19 20.5523 18.5523 21 18 21C17.4872 21 17.0645 20.614 17.0067 20.1166L17 20V4C17 3.44772 17.4477 3 18 3ZM5.29289 3.29289C5.65338 2.93241 6.22061 2.90468 6.6129 3.2097L6.70711 3.29289L14.7071 11.2929C15.0676 11.6534 15.0953 12.2206 14.7903 12.6129L14.7071 12.7071L6.70711 20.7071C6.31658 21.0976 5.68342 21.0976 5.29289 20.7071C4.93241 20.3466 4.90468 19.7794 5.2097 19.3871L5.29289 19.2929L12.5858 12L5.29289 4.70711C4.90237 4.31658 4.90237 3.68342 5.29289 3.29289Z" fill="#212121"/></svg>')
  .current
    //  font-family: $ff-code
    background: var(--theme-highlight)
    color: var(--text-revert)
    &:hover
      color: var(--text-revert)
    //color: var(--text-p3)
  .extend
    color: var(--text-p3)
    padding: 1rem
    line-height: 0
    filter: grayscale(100%)
    img
      height: 1rem
    &.disable
      pointer-events: none
  span.extend
    opacity: 0.25
  a.extend:hover
    filter: unset
